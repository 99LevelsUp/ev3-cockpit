{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "type": "command",
        "powershell": "pwsh -NoProfile -NonInteractive -ExecutionPolicy Bypass -File ./scripts/session-start.ps1",
        "cwd": ".github/hooks",
        "timeoutSec": 10
      }
    ],
    "sessionEnd": [
      {
        "type": "command",
        "powershell": "pwsh -NoProfile -NonInteractive -ExecutionPolicy Bypass -File ./scripts/session-end.ps1",
        "cwd": ".github/hooks",
        "timeoutSec": 10
      }
    ],
    "userPromptSubmitted": [
      {
        "type": "command",
        "powershell": "pwsh -NoProfile -NonInteractive -ExecutionPolicy Bypass -File ./scripts/prompt.ps1",
        "cwd": ".github/hooks",
        "timeoutSec": 10
      }
    ],
    "errorOccurred": [
      {
        "type": "command",
        "powershell": "pwsh -NoProfile -NonInteractive -ExecutionPolicy Bypass -File ./scripts/error.ps1",
        "cwd": ".github/hooks",
        "timeoutSec": 10
      }
    ]
  }
}
